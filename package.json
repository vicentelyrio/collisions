{
  "name": "react-collision",
  "version": "1.0.0",
  "description": "Collision hooks",
  "author": "vicentelyrio@gmail.com",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint --fix",
    "lint:staged": "lint-staged",
    "test": "jest --passWithNoTests",
    "test:coverage": "npm run test --coverage",
    "test:coverage:open": "open coverage/lcov-report/index.html",
    "test:watch": "npm run test --watch",
    "test:ci": "npm run lint && npm run test:coverage",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "semantic-release:local": "semantic-release --dry-run --no-ci --debug=true"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.0",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@vitejs/plugin-react": "^1.0.2",
    "babel-jest": "^27.0.6",
    "babel-plugin-styled-components": "^1.13.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-react-hooks-shallow": "^1.5.1",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.1.2",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "semantic-release": "^19.0.3",
    "vite": "^2.9.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vicentelyrio/react-collision.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vicentelyrio/react-collision/issues"
  },
  "homepage": "https://github.com/vicentelyrio/react-collision#readme",
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
